{
  "version": 2,
  "buildCommand": "cd \"GigCalendar - Frontend\" && npm install && npm run build",
  "outputDirectory": "GigCalendar - Frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "GigCalendar-API/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "GigCalendar - Frontend/package.json",
      "use": "@vercel/static-build"
    }
  ]
}
