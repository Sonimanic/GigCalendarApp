{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "env": {
    "VITE_API_URL": "https://gig-calendar-app-api.vercel.app/api",
    "VITE_WS_URL": "wss://gig-calendar-app-api.vercel.app"
  }
}
